<div class="box">
    <% for(let i=0; i < trending.length; i++){ %>
     <%   if(trending[i].ratings==0 || trending[i].ratings == 'NaN') continue; %>
        <div class="movie-item w3-animate-opacity" onclick="location.href='<%= trending[i].mediaType %>/<%= trending[i].id %>' " style="cursor:pointer;">
          <img class="movie-poster" src="<%= trending[i].posterURL %>" onerror="this.onerror=null; this.src='/images/default.png'">
            <h3><%= trending[i].title %></h3>
            <p><%= trending[i].overview %></p>
            <h6>Ratings: <%= trending[i].ratings %></h6>
        </div> 
    <% } %>   
    </div>